{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "airbnb",
    "rules": {
      "no-underscore-dangle": 0,
      "prefer-destructuring": 0,
      "consistent-return": 0,
      "no-multiple-empty-lines": [
        "error", 
        { "max": 1, "maxBOF": 0, "maxEOF": 0 }
      ],
      "max-len": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "radix": 0,
      "no-unused-vars": 0,
      "no-buffer-constructor": 0,
      "no-useless-concat": 0,
      "object-shorthand": 0
    }
  }